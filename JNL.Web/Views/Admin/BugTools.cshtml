@{
    ViewBag.Title = "BUG修复工具（仅姚智禄登录可见）";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h5 class="title">@ViewBag.Title</h5>

<div class="btn" id="fixRisk">修复综合分析下的风险等级点不开的BUG</div>

<script>
    $(function() {
        $('#fixRisk').on('click',
            function() {
                common.ajax({
                    url: '/Admin/FixRiskSummaryData'
                }).done(function(res) {
                    Materialize('已修复！！！', 3000);
                });
            });
    });
</script>